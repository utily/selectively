{
  "name": "selectively",
  "version": "0.0.69",
  "description": "Create selection by filtering a collection.",
  "author": "PayFunc",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/payfunc/selectively"
  },
  "bugs": {
    "url": "https://github.com/payfunc/selectively/issues"
  },
  "homepage": "https://payfunc.com/open-source/selectively",
  "private": false,
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "git": {
    "tagName": "v${version}"
  },
  "github": {
    "release": true
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.test.json"
      }
    },
    "testEnvironment": "node",
    "testRegex": "((\\.|/)(test|spec))(\\.|\\/.+)(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "node_modules/",
      "dist/"
    ],
    "collectCoverageFrom": [
      "**/*.{ts,tsx,js,jsx}",
      "!**/node_modules/**",
      "!**/dist/**"
    ],
    "preset": "ts-jest",
    "testMatch": null
  },
  "scripts": {
    "dev": "watch 'npm run build' .",
    "lint": "eslint '**/*.{js,ts,tsx}'",
    "fix": "eslint '**/*.{js,ts,tsx}' --fix",
    "build": "tsc -p .",
    "test": "jest --maxWorkers=2",
    "test:watch": "watch jest",
    "prepare": "npm run build",
    "clean": "rm -rf dist node_modules coverage"
  },
  "dependencies": {
    "@cogneco/mend": "^1.7.15"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@types/jest": "^27.4.0",
    "@types/node": "^14.14.20",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "babel-jest": "^27.4.5",
    "eslint": "^7.32.0",
    "eslint-plugin-prettierx": "github:robinnedergard/eslint-plugin-prettierx#master",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^27",
    "prettierx": "github:robinnedergard/prettierx#newline-before-clause-sync",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4",
    "watch": "^0.13.0"
  }
}
