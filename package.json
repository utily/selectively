{
  "name": "selectively",
  "version": "2.0.3",
  "description": "Create selection by filtering a collection.",
  "author": "PayFunc",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/payfunc/selectively"
  },
  "bugs": {
    "url": "https://github.com/payfunc/selectively/issues"
  },
  "homepage": "https://payfunc.com/open-source/selectively",
  "private": false,
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "git": {
    "tagName": "v${version}"
  },
  "github": {
    "release": true
  },
  "jest": {
    "transform": {
      "^.+\\.(j|t)sx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(cryptly|authly|isoly|gracely|cloudly-http|cloudly-router|cloudly-rest)/.*)"
    ],
    "testEnvironment": "node",
    "testRegex": "((\\.|/)(test|spec))(\\.|\\/.+)(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "node_modules/",
      "dist/"
    ],
    "collectCoverageFrom": [
      "**/*.{ts,tsx,js,jsx}",
      "!**/node_modules/**",
      "!**/dist/**"
    ],
    "preset": "ts-jest",
    "testMatch": null
  },
  "scripts": {
    "dev": "watch 'npm run build' .",
    "lint": "eslint '**/*.{js,ts,tsx}'",
    "fix": "eslint '**/*.{js,ts,tsx}' --fix",
    "build": "tsc -p .",
    "test": "jest",
    "test:watch": "watch jest",
    "prepare": "npm run build",
    "clean": "rm -rf dist node_modules coverage"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@cogneco/mend": "^1.7.15"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.32.0",
    "eslint-config-standardize": "^0.9.1",
    "eslint-plugin-prettierx": "github:utily/eslint-plugin-prettierx#utily-20221229",
    "eslint-plugin-simple-import-sort": "^9.0.0",
    "jest": "^29",
    "prettierx": "github:utily/prettierx#utily-20221229",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.4",
    "watch": "^0.13.0"
  }
}
