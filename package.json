{
 "name": "selectively",
 "version": "0.0.67",
 "description": "Create selection by filtering a collection.",
 "author": "PayFunc",
 "license": "MIT",
 "repository": {
  "type": "git",
  "url": "git+https://github.com/payfunc/selectively"
 },
 "bugs": {
  "url": "https://github.com/payfunc/selectively/issues"
 },
 "homepage": "https://payfunc.com/open-source/selectively",
 "private": false,
 "main": "dist/index.js",
 "typings": "dist/index.d.ts",
 "git": {
  "tagName": "v${version}"
 },
 "github": {
  "release": true
 },
 "jest": {
  "globals": {
   "ts-jest": {
    "tsConfig": "tsconfig.test.json"
   }
  },
  "testEnvironment": "node",
  "testRegex": "((\\.|/)(test|spec))(\\.|\\/.+)(jsx?|tsx?)$",
  "testPathIgnorePatterns": [
   "node_modules/",
   "dist/"
  ],
  "collectCoverageFrom": [
   "**/*.{ts,tsx,js,jsx}",
   "!**/node_modules/**",
   "!**/dist/**"
  ],
  "preset": "ts-jest",
  "testMatch": null
 },
 "scripts": {
  "dev": "watch 'npm run build' .",
  "lint": "eslint '**/*.{js,ts,tsx}'",
  "fix": "eslint '**/*.{js,ts,tsx}' --fix",
  "build": "tsc -p .",
  "test": "jest --maxWorkers=2",
  "test:watch": "watch jest",
  "prepare": "npm run build",
  "clean": "rm -rf dist node_modules coverage"
 },
 "dependencies": {
  "@cogneco/mend": "^1.7.15"
 },
 "devDependencies": {
  "@types/jest": "^26.0.0",
  "@types/node": "14.6.2",
  "@typescript-eslint/eslint-plugin": "^3.10.1",
  "@typescript-eslint/parser": "^3.10.1",
  "babel-core": "^6.26.3",
  "babel-jest": "^26.0.1",
  "eslint": "7.8.1",
  "eslint-plugin-prettierx": "github:nilssonemma/eslint-plugin-prettierx#master",
  "eslint-plugin-simple-import-sort": "^7.0.0",
  "jest": "^26.0.1",
  "prettierx": "github:simonmika/prettierx#newline-before-clause",
  "ts-jest": "^26.1.0",
  "typescript": "^3.9.5",
  "watch": "^1.0.2"
 }
}
